<resources>
  <style name="AlertTheme" parent="android:Theme.DeviceDefault.Dialog.Alert">
    <item name="android:background">@color/colorWhite</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:paddingTop">5dp</item>
    <item name="android:paddingBottom">5dp</item>
    <item name="android:paddingLeft">5dp</item>
    <item name="android:paddingRight">5dp</item>
    <item name="android:textSize">24sp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:layout_weight">1</item>
  </style>
  <style name="MyAlertDialogTheme" parent="android:Theme.DeviceDefault.Dialog.Alert">
    <item name="android:windowBackground">@color/black_overlay</item>
    <item name="android:background">@android:color/transparent</item>
    <item name="android:textAppearance">?android:textAppearanceMedium</item>
    <item name="android:lineSpacingMultiplier">1.1</item>
    <item name="android:windowContentOverlay">@null</item>
    <item name="android:windowIsFloating">true</item>
    <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
    <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
  </style>
  <style name="NXKBSyle">
    <item name="android:layout_marginHorizontal">1.3dp</item>
    <item name="android:layout_marginVertical">1.3dp</item>
    <item name="android:background">@drawable/selector</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">17sp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:capitalize">none</item>
    <item name="android:autoText">false</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:cursorVisible">false</item>
    <item name="android:editable">false</item>
    <item name="android:inputType">none</item>
  </style>
  <style name="NXKBSyleN">
    <item name="android:layout_marginHorizontal">0dp</item>
    <item name="android:layout_marginVertical">1.5dp</item>
    <item name="android:background">@drawable/selectorNumeric</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">15sp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:capitalize">none</item>
    <item name="android:autoText">false</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:cursorVisible">false</item>
    <item name="android:editable">false</item>
    <item name="android:inputType">none</item>
  </style>
  <style name="NXKBSyleS">
    <item name="android:background">@drawable/selectorSpace</item>
    <item name="android:textStyle">bold</item>
    <item name="android:textSize">14sp</item>
    <item name="android:textAllCaps">false</item>
    <item name="android:capitalize">none</item>
    <item name="android:autoText">false</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:layout_weight">1</item>
    <item name="android:cursorVisible">false</item>
    <item name="android:editable">false</item>
    <item name="android:inputType">none</item>
  </style>
  <style name="Widget" />
  <style name="Widget.DescriptionBar">
    <item name="android:background">#fb3</item>
    <item name="android:paddingTop">@dimen/margin_medium</item>
    <item name="android:paddingBottom">@dimen/margin_medium</item>
    <item name="android:paddingLeft">@dimen/page_margin</item>
    <item name="android:paddingRight">@dimen/page_margin</item>
    <item name="android:textAppearance">?android:textAppearanceMedium</item>
    <item name="android:lineSpacingMultiplier">1.1</item>
  </style>
  <style name="AppTheme"  />
  <style name="FullscreenTheme">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowBackground">@color/colorPrimary2</item>
    <item name="android:statusBarColor">@color/colorOffWhite</item>
    <item name="android:navigationBarColor">@color/colorPrimary2</item>
    <item name="android:color">@color/colorPrimary2</item>
    <item name="android:windowTranslucentStatus">true</item>
    <item name="android:windowTranslucentNavigation">false</item>
    <item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
    <item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
  </style>
  <style name="FullscreenActionBarStyle">
    <item name="android:background">@color/black_overlay</item>
  </style>
  <color name="DarkGreen">#226034</color>
  <color name="green">#303F9F</color>
  <color name="space">#303F9F</color>
  <color name="black">#020938</color>
  <color name="black3">#020938</color>
  <color name="black2">#303030</color>
  <color name="gray">#587d86</color>
  <color name="gray2">#485e63</color>
  <color name="colorPrimary">#587d86</color>
  <color name="colorPrimary2">#132174</color>
  <color name="colorPrimary3">#d0d1d8</color>
  <color name="colorPrimary4">#bebfc6</color>
  <color name="colorPrimaryDark">#303F9F</color>
  <color name="colorPrimaryDarkD">#303F9F</color>
  <color name="colorAccent">#FF4081</color>
  <color name="black_overlay">#66000000</color>
  <color name="colorOffWhite">#ffffff</color>
  <color name="colorOffWhite2">#f0f0f7</color>
  <color name="colorWhite">#f2f2f9</color>
  <declare-styleable name="ButtonBarContainerTheme">
    <attr name="metaButtonBarStyle" format="reference" />
    <attr name="metaButtonBarButtonStyle" format="reference" />
  </declare-styleable>
</resources>
